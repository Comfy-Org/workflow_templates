{
  "name": "comfyui-template-site",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@9.15.0",
  "scripts": {
    "prepare": "husky || true",
    "dev": "astro dev",
    "locale": "lobe-i18n",
    "sync": "tsx scripts/sync-templates.ts",
    "sync:en-only": "tsx scripts/sync-templates.ts --en-only",
    "sync:locale": "tsx scripts/sync-templates.ts --locale",
    "sync:tutorials": "tsx scripts/sync-tutorials.ts",
    "generate:ai": "tsx scripts/generate-ai.ts",
    "generate:ai:test": "tsx scripts/generate-ai.ts --test",
    "generate:ai:template": "tsx scripts/generate-ai.ts --template",
    "generate:ai:force": "tsx scripts/generate-ai.ts --force",
    "generate:ai:dry-run": "tsx scripts/generate-ai.ts --dry-run",
    "generate:previews": "tsx scripts/generate-previews.ts",
    "cache:status": "tsx scripts/cache-status.ts",
    "cache:clear": "tsx scripts/cache-clear.ts",
    "prebuild": "tsx scripts/prebuild-parallel.ts",
    "prebuild:sequential": "pnpm run sync && pnpm run sync:tutorials && pnpm run generate:ai && pnpm run generate:previews",
    "build": "astro build",
    "build:profile": "tsx scripts/build-profile.ts",
    "preview": "astro preview",
    "astro": "astro",
    "check": "astro check",
    "validate:templates": "tsx scripts/validate-templates.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "research:competitors": "tsx scripts/competitor-keywords.ts",
    "audit:seo": "tsx scripts/seo-audit.ts",
    "validate:sitemap": "tsx scripts/validate-sitemap.ts",
    "research:paa": "tsx scripts/paa-research.ts",
    "test:visual": "playwright test visual.spec.ts",
    "test:visual:update": "playwright test visual.spec.ts --update-snapshots",
    "test:e2e": "playwright test e2e.spec.ts",
    "test:e2e:headed": "playwright test e2e.spec.ts --headed",
    "knip": "knip",
    "knip:fix": "knip --fix"
  },
  "lint-staged": {
    "*.{js,ts,mjs,astro}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml,css}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@astrojs/sitemap": "^3.3.1",
    "@astrojs/vercel": "^9.0.4",
    "@tailwindcss/vite": "^4.1.18",
    "@vercel/analytics": "^1.6.1",
    "astro": "^5.16.11",
    "cheerio": "1.0.0-rc.12",
    "openai": "^4.0.0",
    "tailwindcss": "^4.1.18",
    "web-vitals": "^4.2.4"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.6",
    "@eslint/js": "^9.0.0",
    "@lobehub/i18n-cli": "^1.26.0",
    "@playwright/test": "^1.58.1",
    "@types/node": "^20.0.0",
    "ajv-cli": "^5.0.0",
    "eslint": "^9.0.0",
    "eslint-plugin-astro": "^1.2.0",
    "globals": "^17.3.0",
    "husky": "^9.1.7",
    "knip": "^5.83.0",
    "lint-staged": "^16.2.7",
    "node-html-parser": "^7.0.2",
    "prettier": "^3.2.0",
    "prettier-plugin-astro": "^0.14.0",
    "tsx": "^4.7.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.0.0",
    "vitest": "^4.0.18"
  },
  "optionalDependencies": {
    "canvas": "^2.11.0"
  }
}
