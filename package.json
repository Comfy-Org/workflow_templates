{
  "name": "comfyui-workflow-templates-build",
  "version": "0.2.1",
  "description": "Build tooling for ComfyUI workflow templates",
  "type": "module",
  "scripts": {
    "build": "node scripts/build.js",
    "build:minify-only": "BUILD_MODE=minify-only node scripts/build.js",
    "build:with-hashing": "BUILD_MODE=with-hashing node scripts/build.js",
    "dev": "vite build --watch",
    "validate": "python3 scripts/validate_templates.py"
  },
  "keywords": [
    "comfyui",
    "workflow",
    "templates"
  ],
  "author": "Comfy-Org",
  "license": "MIT",
  "devDependencies": {
    "vite": "^5.4.0"
  }
}
