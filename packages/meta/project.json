{
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/master/packages/nx/schemas/project-schema.json",
  "name": "meta",
  "projectType": "library",
  "sourceRoot": "packages/meta/src",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "packages/meta",
        "command": "python3 -m build"
      },
      "dependsOn": ["sync-assets"]
    },
    "sync-assets": {
      "executor": "nx:run-commands",
      "options": {
        "command": "python scripts/sync_bundles.py"
      }
    }
  },
  "tags": [],
  "implicitDependencies": [
    "core",
    "media-api",
    "media-video",
    "media-image",
    "media-other"
  ]
}
